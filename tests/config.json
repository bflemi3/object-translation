{
  "serverToClient": [
    {
      "source": "pageInfo",
      "properties": [ "page", "pageSize" ]
    },
    {
      "source": "pageInfo.count",
      "target": "count"
    },
    {
      "source": "rows",
      "items": [
        "customer_number",
        {
          "source": "owner",
          "filter": { "find": "^\\s{17}", "replace": "" }
        },
        {
          "target": "system",
          "properties": [
            {
              "source": "mac_address_.*",
              "filter": { "find": "^\\s{4}", "replace": "" }
            },
            {
              "source": "serial_number",
              "target": "serialNumber"
            },
            {
              "source": "system_(.*)",
              "target": "$1",
              "filter": { "find": "^\\s{1}", "replace": "" }
            },
            {
              "target": "constraints",
              "properties": [ ".*_max$", "^max_.*" ]
            },
            {
              "target": "softwareOptions",
              "properties": [
                {
                  "source": "^byte[\\d]+_.*",
                  "filter": [
                    { "find": "Y", "replace": true },
                    { "find": "N", "replace": false }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "clientToServer": [
    "pageInfo",
    {
      "source": "count",
      "target": "pageInfo.count"
    },
    {
      "source": "rows",
      "items": [
        "customer_number",
        {
          "source": "owner",
          "filter": { "startIndex": 0, "replace": "                 " }
        },
        {
          "source": "system",
          "target": [
            {
              "source": "mac_address_.*",
              "filter": { "startIndex": 0, "replace": "    " }
            },
            {
              "source": "serialNumber",
              "target": "serial_number"
            },
            {
              "source": "name",
              "target": "system_name",
              "filter": { "startIndex": 0, "replace": " " }
            },
            {
              "source": "version",
              "target": "system_version",
              "filter": { "startIndex": 0, "replace": " " }
            },
            {
              "source": "constraints",
              "target": [ "location_max", "terminal_max", "^max_.*" ]
            },
            {
              "source": "softwareOptions",
              "target": [
                {
                  "source": "^byte[\\d]+_.*",
                  "filter": [
                    { "find": true, "replace": "Y" },
                    { "find": false, "replace": "N" }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },



    {
      "source": "hair",
      "target": "hair_$1"
    },
    {
      "source": "stats",
      "target": "$1"
    }
  ]
}
