{
  "serverToClient": [
    {
      "source": "pageInfo",
      "properties": [ "page", "pageSize" ]
    },
    {
      "source": "pageInfo.count",
      "target": "count"
    },
    {
      "source": "rows",
      "items": [
        "customer_number",
        "owner",
        {
          "target": "system",
          "properties": [
            "mac_address_.*",
            {
              "source": "serial_number",
              "target": "serialNumber"
            },
            {
              "source": "system_(.*)",
              "target": "$1"
            },
            {
              "target": "constraints",
              "properties": [ ".*_max$", "^max_.*" ]
            },
            {
              "target": "softwareOptions",
              "properties": [ "^byte[\\d]+_.*" ]
            }
          ]
        }
      ]
    }
  ],
  "clientToServer": [
    "pageInfo",
    {
      "source": "count",
      "target": "pageInfo.count"
    },
    {
      "source": "rows",
      "items": [
        "customer_number",
        "owner",
        {
          "source": "system",
          "target": [
            "mac_address_.*",
            {
              "source": "serialNumber",
              "target": "serial_number"
            },
            {
              "source": "name",
              "target": "system_name"
            },
            {
              "source": "version",
              "target": "system_version"
            },
            {
              "source": "constraints",
              "target": [ "location_max", "terminal_max", "^max_.*" ]
            },
            {
              "source": "softwareOptions",
              "target": [ "^byte[\\d]+_.*" ]
            }
          ]
        }
      ]
    },



    {
      "source": "hair",
      "target": "hair_$1"
    },
    {
      "source": "stats",
      "target": "$1"
    }
  ]
}
